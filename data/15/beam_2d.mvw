{ safe_quotes_on }
{ MOD_0 = "beam_2d.h3d" }
{ RES_0 = "beam_2d.h3d" }
*Id("HyperWorks", "8.*")
*BeginPage()
    *Title("Analysis Results", On)
    *Layout(1)
    *BeginAnimator(Static)
        *CurrentPosition(0)
        *NumberOfSteps(4)
        *Increment(Forward, Frame, 1, BounceOff)
    *EndAnimator()
    *BeginWindow(Animation)
        *BeginGraphic()
            *BeginModel({MOD_0}, "Hyper3D Reader", 1, 1)
                *ResultSource({RES_0}, "Hyper3D Reader", 1, 1)
                *BeginPart(1, "Global", "PART", 1)
                    *Attribute(On, IdOff, 8, Opa, Sha)
                *EndPart()
                *BeginPart(3, "Orphan-Grids", "PART", 0)
                    *Attribute(Off, IdOff, 37, Opa, Sha)
                *EndPart()
                *BeginGroup("Orphan Nodes", "Off", "Off", "255 255 255", 1, "wire")
                    *BeginSelection(Part, SelectAll, "User_Set", 3)
                        *Add("Orphan-Grids id 1")
                    *EndSelection()
                *EndGroup()
                *BeginMask()
                    *BeginSelection(Part, SelectAll, "User_Set", 0)
                        *Add("Orphan-Grids id 1")
                    *EndSelection()
                *EndMask()
            *EndModel()
            *CurrentModel(0)
            *View(Isometric)
        *EndGraphic()
    *EndWindow()
*EndPage()
